//13.Write a Program to Reverse The String "Welcome To Java"?

Public class Reverse {
    public static void main(String[] args) {
    Reverse obj=new Reverse();
   obj.reverse_string();
}
public void reverse_string(){
        String name="Welcome To Java";
        String reversed=" ";
        System.out.println(" The string name is ==>"+name);
        for(int i=0;i<name.length();i++) {
            reversed = name.charAt(i) + reversed;
        }
        System.out.println("The Reversed string==>"+reversed);
        }

    }
