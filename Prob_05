//Prob 5 :  Run a for loop from 500 to 1000 where if loop finds number 808 then it will print the number found as 808

import java.io.*;
public class Loopcondition {
    public static void main(String[] args) {
        int x=1000;
        for (int i=500; i<=x; i++) {
            int y=i;
            if (y==808)
                System.out.println("The number finds"+i);
        }
    }
 }
