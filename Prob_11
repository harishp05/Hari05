// Prob 11 :If EmpNo=123 then EmpName=Jadeja. Like this how can we keep 100 EmpName with its EmpId?

import java.util.HashMap;
import java.util.Map;

public class EmployeeRecords {
    public static void main(String[] args) {
        // Create a HashMap to store employee records
        Map<Integer, String> employeeRecords = new HashMap<>();

        // Add employee records to the HashMap
        employeeRecords.put(123, "Jadeja");
        employeeRecords.put(456, "Smith");
        employeeRecords.put(789, "Johnson");
        // Add more employee records here...

        // Retrieve and print an employee's name using their ID
        int empId = 456;
        String empName = employeeRecords.get(empId);
        System.out.println("Employee ID: " + empId + ", Employee Name: " + empName);
    }
}
